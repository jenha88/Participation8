<Window x:Class="GOTQuote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOTQuote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="http://cdn2.vox-cdn.com/assets/4230883/The_Wall_from_the_south.jpg"/>
        </Grid.Background>
        <Button x:Name="btbgen" Content="Generate" HorizontalAlignment="Left" Margin="443,224,0,0" VerticalAlignment="Top" Width="98" Click="btbgen_Click"/>
        <TextBlock x:Name="txtblock" HorizontalAlignment="Left" Margin="10,224,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="186" Width="380" Background="#FFCAC1C1"/>
        <Button x:Name="btbexp" Content="Export" HorizontalAlignment="Left" Margin="627,224,0,0" VerticalAlignment="Top" Width="95" Click="btbexp_Click"/>
        <Label Content="Game of Thrones Quote " HorizontalAlignment="Left" Margin="205,33,0,0" VerticalAlignment="Top" FontSize="35" Background="#FFEEE7E7"/>

    </Grid>
</Window>
